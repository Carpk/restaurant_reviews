<div class="row">

  <div class="feature-box col-sm-12">
    <h1>Feature</h1>

    <h1>Latest</h1>

    <%= link_to @latest.title, restaurant_review_path(@latest.restaurant.name, @latest.title) %><br>
    <%= link_to @latest.restaurant.name, restaurant_path(@latest.restaurant.name) %>
  </div>


  <div class="feature-box col-sm-12">
    <h1>Categories</h1>
    <% @categories.each do |category| %>
      <%= link_to category.set, category_path(category.set) %>
    <% end %>
  </div>
</div>
